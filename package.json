{
  "name": "unplugin-starter",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.28.0",
  "description": "Description.",
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sxzz",
  "homepage": "https://github.com/sxzz/unplugin-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-starter.git"
  },
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-starter/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rspack",
    "rollup",
    "rolldown",
    "esbuild",
    "farm"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./api": "./dist/api.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./rolldown": "./dist/rolldown.js",
    "./rollup": "./dist/rollup.js",
    "./rspack": "./dist/rspack.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "unplugin": "^2.3.11"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.4.5",
    "@sxzz/prettier-config": "^2.2.6",
    "@sxzz/test-utils": "^0.5.15",
    "@types/node": "^25.0.8",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsdown": "^0.20.0-beta.2",
    "tsdown-preset-sxzz": "^0.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "prettier": "@sxzz/prettier-config"
}
